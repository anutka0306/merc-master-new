    {{ include('v2/blocks/section/hero.html.twig',{ h1 : block('h1') ?? page.h1, photoBig : page.priceBrand.photoBig, brandName : page.priceBrand.name }) }}
{% endblock %}

{% block body %}
    {{ include('elements/_content.html.twig') }}
