{% extends 'v2/base.html.twig' %}

{% block body %}
    {% embed 'v2/embed/content.html.twig' %}
        {% block content %}
            <h2>Все вакансии</h2>
            <ul>
                {% for item in items %}
                    <li>
                        <a href="{{ item.path }}">{{ item.name }}</a>
                    </li>
                {% endfor %}
            </ul>
        {% endblock %}
    {% endembed %}
{% endblock %}
